# from .Values.conf.neutron.db.allow_automatic_l3agent_failover
{{ if not .Values.conf.neutron.db.allow_automatic_l3agent_failover }}#{{ end }}allow_automatic_l3agent_failover = {{ .Values.conf.neutron.db.allow_automatic_l3agent_failover | default "false" }}

# Enable HA mode for virtual routers. (boolean value)
