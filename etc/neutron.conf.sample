# from .Values.conf.neutron.db.l3_ha_network_type
{{ if not .Values.conf.neutron.db.l3_ha_network_type }}#{{ end }}l3_ha_network_type = {{ .Values.conf.neutron.db.l3_ha_network_type | default "" }}

# The physical network name with which the HA network can be created. (string
# value)
# from .Values.conf.neutron.db.l3_ha_network_physical_name
{{ if not .Values.conf.neutron.db.l3_ha_network_physical_name }}#{{ end }}l3_ha_network_physical_name = {{ .Values.conf.neutron.db.l3_ha_network_physical_name | default "" }}

#
# From neutron.extensions
#

# Maximum number of allowed address pairs (integer value)
# from .Values.conf.neutron.extensions.max_allowed_address_pair
{{ if not .Values.conf.neutron.extensions.max_allowed_address_pair }}#{{ end }}max_allowed_address_pair = {{ .Values.conf.neutron.extensions.max_allowed_address_pair | default "10" }}

#
# From oslo.log
