[DEFAULT]

#
# From neutron
#

# Where to store Neutron state files. This directory must be writable by the
# agent. (string value)
# from .Values.conf.neutron.state_path
{{ if not .Values.conf.neutron.state_path }}#{{ end }}state_path = {{ .Values.conf.neutron.state_path | default "/var/lib/neutron" }}

# The host IP to bind to (string value)
# from .Values.conf.neutron.bind_host
{{ if not .Values.conf.neutron.bind_host }}#{{ end }}bind_host = {{ .Values.conf.neutron.bind_host | default "0.0.0.0" }}

# The port to bind to (port value)
# Minimum value: 0
# Maximum value: 65535
# from .Values.conf.neutron.bind_port
{{ if not .Values.conf.neutron.bind_port }}#{{ end }}bind_port = {{ .Values.conf.neutron.bind_port | default "9696" }}

# The path for API extensions. Note that this can be a colon-separated list of
# paths. For example: api_extensions_path =
# extensions:/path/to/more/exts:/even/more/exts. The __path__ of
# neutron.extensions is appended to this, so if your extensions are in there
# you don't need to specify them here. (string value)
# from .Values.conf.neutron.api_extensions_path
{{ if not .Values.conf.neutron.api_extensions_path }}#{{ end }}api_extensions_path = {{ .Values.conf.neutron.api_extensions_path | default "" }}

# The type of authentication to use (string value)
# from .Values.conf.neutron.auth_strategy
{{ if not .Values.conf.neutron.auth_strategy }}#{{ end }}auth_strategy = {{ .Values.conf.neutron.auth_strategy | default "keystone" }}

# The core plugin Neutron will use (string value)
# from .Values.conf.neutron.core_plugin
{{ if not .Values.conf.neutron.core_plugin }}#{{ end }}core_plugin = {{ .Values.conf.neutron.core_plugin | default "<None>" }}

# The service plugins Neutron will use (list value)
# from .Values.conf.neutron.service_plugins
{{ if not .Values.conf.neutron.service_plugins }}#{{ end }}service_plugins = {{ .Values.conf.neutron.service_plugins | default "" }}

# The base MAC address Neutron will use for VIFs. The first 3 octets will
# remain unchanged. If the 4th octet is not 00, it will also be used. The
# others will be randomly generated. (string value)
# from .Values.conf.neutron.base_mac
{{ if not .Values.conf.neutron.base_mac }}#{{ end }}base_mac = {{ .Values.conf.neutron.base_mac | default "fa:16:3e:00:00:00" }}

# Allow the usage of the bulk API (boolean value)
# from .Values.conf.neutron.allow_bulk
{{ if not .Values.conf.neutron.allow_bulk }}#{{ end }}allow_bulk = {{ .Values.conf.neutron.allow_bulk | default "true" }}

# The maximum number of items returned in a single response, value was
# 'infinite' or negative integer means no limit (string value)
# from .Values.conf.neutron.pagination_max_limit
{{ if not .Values.conf.neutron.pagination_max_limit }}#{{ end }}pagination_max_limit = {{ .Values.conf.neutron.pagination_max_limit | default "-1" }}

# Default value of availability zone hints. The availability zone aware
# schedulers use this when the resources availability_zone_hints is empty.
# Multiple availability zones can be specified by a comma separated string.
# This value can be empty. In this case, even if availability_zone_hints for a
# resource is empty, availability zone is considered for high availability
# while scheduling the resource. (list value)
# from .Values.conf.neutron.default_availability_zones
{{ if not .Values.conf.neutron.default_availability_zones }}#{{ end }}default_availability_zones = {{ .Values.conf.neutron.default_availability_zones | default "" }}

# Maximum number of DNS nameservers per subnet (integer value)
# from .Values.conf.neutron.max_dns_nameservers
{{ if not .Values.conf.neutron.max_dns_nameservers }}#{{ end }}max_dns_nameservers = {{ .Values.conf.neutron.max_dns_nameservers | default "5" }}

# Maximum number of host routes per subnet (integer value)
# from .Values.conf.neutron.max_subnet_host_routes
{{ if not .Values.conf.neutron.max_subnet_host_routes }}#{{ end }}max_subnet_host_routes = {{ .Values.conf.neutron.max_subnet_host_routes | default "20" }}

# DEPRECATED: Maximum number of fixed ips per port. This option is deprecated
# and will be removed in the Ocata release. (integer value)
# This option is deprecated for removal.
# Its value may be silently ignored in the future.
# from .Values.conf.neutron.max_fixed_ips_per_port
{{ if not .Values.conf.neutron.max_fixed_ips_per_port }}#{{ end }}max_fixed_ips_per_port = {{ .Values.conf.neutron.max_fixed_ips_per_port | default "5" }}

# Enables IPv6 Prefix Delegation for automatic subnet CIDR allocation. Set to
# True to enable IPv6 Prefix Delegation for subnet allocation in a PD-capable
# environment. Users making subnet creation requests for IPv6 subnets without
# providing a CIDR or subnetpool ID will be given a CIDR via the Prefix
# Delegation mechanism. Note that enabling PD will override the behavior of the
# default IPv6 subnetpool. (boolean value)
# from .Values.conf.neutron.ipv6_pd_enabled
{{ if not .Values.conf.neutron.ipv6_pd_enabled }}#{{ end }}ipv6_pd_enabled = {{ .Values.conf.neutron.ipv6_pd_enabled | default "false" }}

# DHCP lease duration (in seconds). Use -1 to tell dnsmasq to use infinite
# lease times. (integer value)
# from .Values.conf.neutron.dhcp_lease_duration
{{ if not .Values.conf.neutron.dhcp_lease_duration }}#{{ end }}dhcp_lease_duration = {{ .Values.conf.neutron.dhcp_lease_duration | default "86400" }}

# Domain to use for building the hostnames (string value)
# from .Values.conf.neutron.dns_domain
{{ if not .Values.conf.neutron.dns_domain }}#{{ end }}dns_domain = {{ .Values.conf.neutron.dns_domain | default "openstacklocal" }}

# Driver for external DNS integration. (string value)
# from .Values.conf.neutron.external_dns_driver
{{ if not .Values.conf.neutron.external_dns_driver }}#{{ end }}external_dns_driver = {{ .Values.conf.neutron.external_dns_driver | default "<None>" }}

# Allow sending resource operation notification to DHCP agent (boolean value)
# from .Values.conf.neutron.dhcp_agent_notification
{{ if not .Values.conf.neutron.dhcp_agent_notification }}#{{ end }}dhcp_agent_notification = {{ .Values.conf.neutron.dhcp_agent_notification | default "true" }}

# Allow overlapping IP support in Neutron. Attention: the following parameter
# MUST be set to False if Neutron is being used in conjunction with Nova
# security groups. (boolean value)
# from .Values.conf.neutron.allow_overlapping_ips
{{ if not .Values.conf.neutron.allow_overlapping_ips }}#{{ end }}allow_overlapping_ips = {{ .Values.conf.neutron.allow_overlapping_ips | default "false" }}

# Hostname to be used by the Neutron server, agents and services running on
# this machine. All the agents and services running on this machine must use
# the same host value. (string value)
# from .Values.conf.neutron.host
{{ if not .Values.conf.neutron.host }}#{{ end }}host = {{ .Values.conf.neutron.host | default "example.domain" }}

# Send notification to nova when port status changes (boolean value)
# from .Values.conf.neutron.notify_nova_on_port_status_changes
{{ if not .Values.conf.neutron.notify_nova_on_port_status_changes }}#{{ end }}notify_nova_on_port_status_changes = {{ .Values.conf.neutron.notify_nova_on_port_status_changes | default "true" }}

# Send notification to nova when port data (fixed_ips/floatingip) changes so
# nova can update its cache. (boolean value)
# from .Values.conf.neutron.notify_nova_on_port_data_changes
{{ if not .Values.conf.neutron.notify_nova_on_port_data_changes }}#{{ end }}notify_nova_on_port_data_changes = {{ .Values.conf.neutron.notify_nova_on_port_data_changes | default "true" }}

# Number of seconds between sending events to nova if there are any events to
# send. (integer value)
# from .Values.conf.neutron.send_events_interval
{{ if not .Values.conf.neutron.send_events_interval }}#{{ end }}send_events_interval = {{ .Values.conf.neutron.send_events_interval | default "2" }}

# Neutron IPAM (IP address management) driver to use. By default, the reference
# implementation of the Neutron IPAM driver is used. (string value)
# from .Values.conf.neutron.ipam_driver
{{ if not .Values.conf.neutron.ipam_driver }}#{{ end }}ipam_driver = {{ .Values.conf.neutron.ipam_driver | default "internal" }}

# If True, then allow plugins that support it to create VLAN transparent
# networks. (boolean value)
# from .Values.conf.neutron.vlan_transparent
{{ if not .Values.conf.neutron.vlan_transparent }}#{{ end }}vlan_transparent = {{ .Values.conf.neutron.vlan_transparent | default "false" }}

# This will choose the web framework in which to run the Neutron API server.
# 'pecan' is a new experimental rewrite of the API server. (string value)
# Allowed values: legacy, pecan
# from .Values.conf.neutron.web_framework
{{ if not .Values.conf.neutron.web_framework }}#{{ end }}web_framework = {{ .Values.conf.neutron.web_framework | default "legacy" }}

# MTU of the underlying physical network. Neutron uses this value to calculate
# MTU for all virtual network components. For flat and VLAN networks, neutron
# uses this value without modification. For overlay networks such as VXLAN,
# neutron automatically subtracts the overlay protocol overhead from this
# value. Defaults to 1500, the standard value for Ethernet. (integer value)
# Deprecated group/name - [ml2]/segment_mtu
# from .Values.conf.neutron.global_physnet_mtu
{{ if not .Values.conf.neutron.global_physnet_mtu }}#{{ end }}global_physnet_mtu = {{ .Values.conf.neutron.global_physnet_mtu | default "1500" }}

# Number of backlog requests to configure the socket with (integer value)
# from .Values.conf.neutron.backlog
{{ if not .Values.conf.neutron.backlog }}#{{ end }}backlog = {{ .Values.conf.neutron.backlog | default "4096" }}

# Number of seconds to keep retrying to listen (integer value)
# from .Values.conf.neutron.retry_until_window
{{ if not .Values.conf.neutron.retry_until_window }}#{{ end }}retry_until_window = {{ .Values.conf.neutron.retry_until_window | default "30" }}

# Enable SSL on the API server (boolean value)
# from .Values.conf.neutron.use_ssl
{{ if not .Values.conf.neutron.use_ssl }}#{{ end }}use_ssl = {{ .Values.conf.neutron.use_ssl | default "false" }}

# Seconds between running periodic tasks. (integer value)
# from .Values.conf.neutron.periodic_interval
{{ if not .Values.conf.neutron.periodic_interval }}#{{ end }}periodic_interval = {{ .Values.conf.neutron.periodic_interval | default "40" }}

# Number of separate API worker processes for service. If not specified, the
# default is equal to the number of CPUs available for best performance.
# (integer value)
# from .Values.conf.neutron.api_workers
{{ if not .Values.conf.neutron.api_workers }}#{{ end }}api_workers = {{ .Values.conf.neutron.api_workers | default "<None>" }}

# Number of RPC worker processes for service. (integer value)
# from .Values.conf.neutron.rpc_workers
{{ if not .Values.conf.neutron.rpc_workers }}#{{ end }}rpc_workers = {{ .Values.conf.neutron.rpc_workers | default "1" }}

# Number of RPC worker processes dedicated to state reports queue. (integer
# value)
# from .Values.conf.neutron.rpc_state_report_workers
{{ if not .Values.conf.neutron.rpc_state_report_workers }}#{{ end }}rpc_state_report_workers = {{ .Values.conf.neutron.rpc_state_report_workers | default "1" }}

# Range of seconds to randomly delay when starting the periodic task scheduler
# to reduce stampeding. (Disable by setting to 0) (integer value)
# from .Values.conf.neutron.periodic_fuzzy_delay
{{ if not .Values.conf.neutron.periodic_fuzzy_delay }}#{{ end }}periodic_fuzzy_delay = {{ .Values.conf.neutron.periodic_fuzzy_delay | default "5" }}

#
# From neutron.agent
#

# The driver used to manage the virtual interface. (string value)
# from .Values.conf.neutron.agent.interface_driver
{{ if not .Values.conf.neutron.agent.interface_driver }}#{{ end }}interface_driver = {{ .Values.conf.neutron.agent.interface_driver | default "<None>" }}

# Location for Metadata Proxy UNIX domain socket. (string value)
# from .Values.conf.neutron.agent.metadata_proxy_socket
{{ if not .Values.conf.neutron.agent.metadata_proxy_socket }}#{{ end }}metadata_proxy_socket = {{ .Values.conf.neutron.agent.metadata_proxy_socket | default "$state_path/metadata_proxy" }}

# User (uid or name) running metadata proxy after its initialization (if empty:
# agent effective user). (string value)
# from .Values.conf.neutron.agent.metadata_proxy_user
{{ if not .Values.conf.neutron.agent.metadata_proxy_user }}#{{ end }}metadata_proxy_user = {{ .Values.conf.neutron.agent.metadata_proxy_user | default "" }}

# Group (gid or name) running metadata proxy after its initialization (if
# empty: agent effective group). (string value)
# from .Values.conf.neutron.agent.metadata_proxy_group
{{ if not .Values.conf.neutron.agent.metadata_proxy_group }}#{{ end }}metadata_proxy_group = {{ .Values.conf.neutron.agent.metadata_proxy_group | default "" }}

# Enable/Disable log watch by metadata proxy. It should be disabled when
# metadata_proxy_user/group is not allowed to read/write its log file and
# copytruncate logrotate option must be used if logrotate is enabled on
# metadata proxy log files. Option default value is deduced from
# metadata_proxy_user: watch log is enabled if metadata_proxy_user is agent
# effective user id/name. (boolean value)
# from .Values.conf.neutron.agent.metadata_proxy_watch_log
{{ if not .Values.conf.neutron.agent.metadata_proxy_watch_log }}#{{ end }}metadata_proxy_watch_log = {{ .Values.conf.neutron.agent.metadata_proxy_watch_log | default "<None>" }}

#
# From neutron.db
#

# Representing the resource type whose load is being reported by the agent.
# This can be "networks", "subnets" or "ports". When specified (Default is
# networks), the server will extract particular load sent as part of its agent
# configuration object from the agent report state, which is the number of
# resources being consumed, at every report_interval.dhcp_load_type can be used
# in combination with network_scheduler_driver =
# neutron.scheduler.dhcp_agent_scheduler.WeightScheduler When the
# network_scheduler_driver is WeightScheduler, dhcp_load_type can be configured
# to represent the choice for the resource being balanced. Example:
# dhcp_load_type=networks (string value)
# Allowed values: networks, subnets, ports
# from .Values.conf.neutron.db.dhcp_load_type
{{ if not .Values.conf.neutron.db.dhcp_load_type }}#{{ end }}dhcp_load_type = {{ .Values.conf.neutron.db.dhcp_load_type | default "networks" }}

# Agent starts with admin_state_up=False when enable_new_agents=False. In the
# case, user's resources will not be scheduled automatically to the agent until
# admin changes admin_state_up to True. (boolean value)
# from .Values.conf.neutron.db.enable_new_agents
{{ if not .Values.conf.neutron.db.enable_new_agents }}#{{ end }}enable_new_agents = {{ .Values.conf.neutron.db.enable_new_agents | default "true" }}

# Maximum number of routes per router (integer value)
# from .Values.conf.neutron.db.max_routes
{{ if not .Values.conf.neutron.db.max_routes }}#{{ end }}max_routes = {{ .Values.conf.neutron.db.max_routes | default "30" }}

# Define the default value of enable_snat if not provided in
# external_gateway_info. (boolean value)
# from .Values.conf.neutron.db.enable_snat_by_default
{{ if not .Values.conf.neutron.db.enable_snat_by_default }}#{{ end }}enable_snat_by_default = {{ .Values.conf.neutron.db.enable_snat_by_default | default "true" }}

# Driver to use for scheduling network to DHCP agent (string value)
# from .Values.conf.neutron.db.network_scheduler_driver
{{ if not .Values.conf.neutron.db.network_scheduler_driver }}#{{ end }}network_scheduler_driver = {{ .Values.conf.neutron.db.network_scheduler_driver | default "neutron.scheduler.dhcp_agent_scheduler.WeightScheduler" }}

# Allow auto scheduling networks to DHCP agent. (boolean value)
# from .Values.conf.neutron.db.network_auto_schedule
{{ if not .Values.conf.neutron.db.network_auto_schedule }}#{{ end }}network_auto_schedule = {{ .Values.conf.neutron.db.network_auto_schedule | default "true" }}

# Automatically remove networks from offline DHCP agents. (boolean value)
# from .Values.conf.neutron.db.allow_automatic_dhcp_failover
{{ if not .Values.conf.neutron.db.allow_automatic_dhcp_failover }}#{{ end }}allow_automatic_dhcp_failover = {{ .Values.conf.neutron.db.allow_automatic_dhcp_failover | default "true" }}

# Number of DHCP agents scheduled to host a tenant network. If this number is
# greater than 1, the scheduler automatically assigns multiple DHCP agents for
# a given tenant network, providing high availability for DHCP service.
# (integer value)
# from .Values.conf.neutron.db.dhcp_agents_per_network
{{ if not .Values.conf.neutron.db.dhcp_agents_per_network }}#{{ end }}dhcp_agents_per_network = {{ .Values.conf.neutron.db.dhcp_agents_per_network | default "1" }}

# Enable services on an agent with admin_state_up False. If this option is
# False, when admin_state_up of an agent is turned False, services on it will
# be disabled. Agents with admin_state_up False are not selected for automatic
# scheduling regardless of this option. But manual scheduling to such agents is
# available if this option is True. (boolean value)
# from .Values.conf.neutron.db.enable_services_on_agents_with_admin_state_down
{{ if not .Values.conf.neutron.db.enable_services_on_agents_with_admin_state_down }}#{{ end }}enable_services_on_agents_with_admin_state_down = {{ .Values.conf.neutron.db.enable_services_on_agents_with_admin_state_down | default "false" }}

# The base mac address used for unique DVR instances by Neutron. The first 3
# octets will remain unchanged. If the 4th octet is not 00, it will also be
# used. The others will be randomly generated. The 'dvr_base_mac' *must* be
# different from 'base_mac' to avoid mixing them up with MAC's allocated for
# tenant ports. A 4 octet example would be dvr_base_mac = fa:16:3f:4f:00:00.
# The default is 3 octet (string value)
# from .Values.conf.neutron.db.dvr_base_mac
{{ if not .Values.conf.neutron.db.dvr_base_mac }}#{{ end }}dvr_base_mac = {{ .Values.conf.neutron.db.dvr_base_mac | default "fa:16:3f:00:00:00" }}

# System-wide flag to determine the type of router that tenants can create.
# Only admin can override. (boolean value)
# from .Values.conf.neutron.db.router_distributed
{{ if not .Values.conf.neutron.db.router_distributed }}#{{ end }}router_distributed = {{ .Values.conf.neutron.db.router_distributed | default "false" }}

# Driver to use for scheduling router to a default L3 agent (string value)
# from .Values.conf.neutron.db.router_scheduler_driver
{{ if not .Values.conf.neutron.db.router_scheduler_driver }}#{{ end }}router_scheduler_driver = {{ .Values.conf.neutron.db.router_scheduler_driver | default "neutron.scheduler.l3_agent_scheduler.LeastRoutersScheduler" }}

# Allow auto scheduling of routers to L3 agent. (boolean value)
# from .Values.conf.neutron.db.router_auto_schedule
{{ if not .Values.conf.neutron.db.router_auto_schedule }}#{{ end }}router_auto_schedule = {{ .Values.conf.neutron.db.router_auto_schedule | default "true" }}

# Automatically reschedule routers from offline L3 agents to online L3 agents.
