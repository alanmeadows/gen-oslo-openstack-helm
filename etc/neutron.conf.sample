{{ if not .Values.conf.neutron.db.max_l3_agents_per_router }}#{{ end }}max_l3_agents_per_router = {{ .Values.conf.neutron.db.max_l3_agents_per_router | default "3" }}

# Subnet used for the l3 HA admin network. (string value)
# from .Values.conf.neutron.db.l3_ha_net_cidr
{{ if not .Values.conf.neutron.db.l3_ha_net_cidr }}#{{ end }}l3_ha_net_cidr = {{ .Values.conf.neutron.db.l3_ha_net_cidr | default "169.254.192.0/18" }}

# The network type to use when creating the HA network for an HA router. By
